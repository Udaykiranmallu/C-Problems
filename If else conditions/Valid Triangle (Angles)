#include <stdio.h>

int main() {
    int a, b, c;

    
    printf("Enter three angles: ");
    scanf("%d %d %d", &a, &b, &c);

   
    if (a > 0 && b > 0 && c > 0 && (a + b + c == 180)) {
        printf("Valid triangle\n");
    }
    else {
        printf("Invalid triangle\n");
    }

    return 0;
}
