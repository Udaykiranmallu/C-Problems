#include <stdio.h>
int main() {
    int mark1,mark2,mark3,mark4,mark5,mark6;
    printf("enter the marks (out of 100)");
    scanf("%d %d %d %d %d %d",&mark1,&mark2,&mark3,&mark4,&mark5,&mark6);
    
    
    int totalmarks,percentage;
    totalmarks=mark1+mark2+mark3+mark4+mark5+mark6;
    percentage=totalmarks*100/600;
    if(percentage>=95 &&percentage<=100){
        printf("Grade A");
    }else if (percentage>=85&&percentage<=94){
        printf("Grade B");
    }
    else if (percentage>=75&&percentage<=84){
        printf("Grade C");
    }
    else if (percentage>=65&&percentage<=74){
        printf("Grade D");
    }
    else if (percentage>=45&&percentage<=64){
        printf("Grade E");
    }
      else if(percentage>0&&percentage<44){
        printf("Grade F");
      }
      else{
        printf("Invalid marks");
      }


    
    return 0;
}
