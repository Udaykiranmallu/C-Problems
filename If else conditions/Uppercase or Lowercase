#include <stdio.h>

int main() {
    char ch;

    // Input
    printf("Enter a character: ");
    scanf("%c", &ch);

    // Check using ASCII range
    if (ch >= 'A' && ch <= 'Z') {
        printf("Uppercase letter\n");
    }
    else if (ch >= 'a' && ch <= 'z') {
        printf("Lowercase letter\n");
    }
    else {
        printf("Not an alphabet letter\n");
    }

    return 0;
}
