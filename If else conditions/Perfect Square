#include<stdio.h>
#include<math.h>
int main(){
    int num;
  ;
    printf("Enter the number :");
    scanf("%d",&num);
   
int root = sqrt(num);
    if(root*root==num){
        printf("%d is a perfect square .\n");
    }
    else{
        printf("%d is not a perfect square .\n");
    }
    return 0;
}
