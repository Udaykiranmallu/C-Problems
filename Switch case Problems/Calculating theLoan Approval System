#include <stdio.h>

int main() {
    int loanType, creditScore;

    // Input
    scanf("%d", &loanType);
    scanf("%d", &creditScore);

    switch(loanType) {
        case 1:  // Home Loan
            if (creditScore >= 700) {
                printf("Approved\n");
            } 
            else if (creditScore >= 650 && creditScore <= 699) {
                printf("Manual Review\n");
            } 
            else {
                printf("Rejected\n");
            }
            break;

        case 2:  // Personal Loan
            if (creditScore >= 700) {
                printf("Approved\n");
            } 
            else {
                printf("Rejected\n");
            }
            break;

        default:
            printf("Invalid Loan Type\n");
    }

    return 0;
}
