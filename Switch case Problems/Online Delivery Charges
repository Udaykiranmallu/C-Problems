#include <stdio.h>

int main() {
    int orderAmount, speedType;
    int deliveryCharge = 0;

    scanf("%d", &orderAmount);
    scanf("%d", &speedType);

    switch(speedType) {
        case 1:  // Normal
            deliveryCharge = 50;
            break;

        case 2:  // Express
            if(orderAmount < 1000) {
                deliveryCharge = 100;
            } else {
                deliveryCharge = 0;   // Free delivery
            }
            break;

        default:
            printf("Invalid Speed Type");
            return 0;
    }

    printf("Delivery %d", deliveryCharge);

    return 0;
}
